{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "buildCommand": "./vercel-build.sh"
      }
    }
  ],
  "routes": [
    {
      "src": "/attached_assets/(.*)",
      "dest": "/attached_assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "outputDirectory": ".vercel/output"
}